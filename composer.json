{
  "name": "gridonic/profiler-bundle",
  "description": "Symfony bundle supporting simple storage of custom profiling data for live applications",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Gridonic AG",
      "email": "hello@gridonic.ch"
    }
  ],
  "keywords": [
    "profiler bundle"
  ],
  "license": [
    "MIT"
  ],
  "require": {
    "php": ">=7.0.0",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/orm": "^2.5",
    "incenteev/composer-parameter-handler": "^2.0",
    "sensio/distribution-bundle": "^5.0.19",
    "sensio/framework-extra-bundle": "^3.0.2",
    "symfony/monolog-bundle": "^3.1.0",
    "symfony/polyfill-apcu": "^1.0",
    "symfony/swiftmailer-bundle": "^2.3.10",
    "symfony/symfony": "3.3.*",
    "twig/twig": "^1.0||^2.0"
  },
  "autoload": {
    "psr-4": { "Gridonic\\ProfilerBundle\\": "" }
  },
  "target-dir": "Company/DemoBundle",
  "extra": {
    "branch-alias": {
      "dev-master": "some_version-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3"
  }
}
